                      "agent_scope_database": [
                        {
                          "type": "Core",
                          "doc_no": "A.6.1.1.6.2.2.2.2.1.2",
                          "name": "Section 2",
                          "uuid": "7db74e26-9f2e-41ba-ac1b-2475168fd207",
